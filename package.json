{
  "name": "gridview",
  "version": "1.0.0",
  "description": "masonry grid layout image viewer",
  "author": "0000xFFFF",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-packager . gridview --platform=linux --arch=x64 --out=release-builds --icon=assets/icon.png"
  },
  "build": {
    "appId": "com.0000xFFFF.gridview",
    "linux": {
      "icon": "assets/icon.png",
      "target": [
        "AppImage"
      ]
    }
  },
  "devDependencies": {
    "@electron/packager": "^18.3.5",
    "electron": "^32.1.2"
  },
  "keywords": [],
  "license": "ISC"
}
